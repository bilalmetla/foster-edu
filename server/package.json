{
  "name": "kello",
  "version": "1.0.0",
  "description": "its online store for fresh vegitables and fruites",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t kello .",
    "docker:run": "docker run -p 3000:3000 -d kello",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register . ",
    "start:watch": "nodemon --watch server --ext ts -r source-map-support/register . ",
    "prepublishOnly": "npm run test",
    "publish:all": "cd ../../dev_admin_portal/ && npm run build && cd ../dev_server/kello/ && cp -r dist /home/bilalmetla/work/gitlab/code/kello && cp -r public /home/bilalmetla/work/gitlab/code/kello",
    "publish:server": "cp -r dist /home/bilalmetla/work/gitlab/code/kello && cp -r public /home/bilalmetla/work/gitlab/code/kello",
    "publish:website": "cp public/index.html /home/bilalmetla/work/gitlab/code/kello && cp -r public/wordpress /home/bilalmetla/work/gitlab/code/kello"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^3.3.0",
    "@loopback/authentication-passport": "^1.1.0",
    "@loopback/boot": "^1.6.0",
    "@loopback/context": "^1.25.0",
    "@loopback/core": "^1.12.0",
    "@loopback/example-log-extension": "^1.2.22",
    "@loopback/openapi-v3": "^1.10.3",
    "@loopback/repository": "^1.16.0",
    "@loopback/rest": "^1.25.0",
    "@loopback/rest-explorer": "^1.4.6",
    "@loopback/service-proxy": "^1.3.13",
    "@types/app-root-path": "^1.2.4",
    "app-root-path": "^3.0.0",
    "firebase-admin": "^8.9.2",
    "jsonwebtoken": "^8.5.1",
    "loopback-connector-mongodb": "^4.2.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "twilio": "^3.39.3",
    "uuid": "^3.3.3",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  },
  "devDependencies": {
    "@loopback/build": "^3.0.0",
    "@loopback/eslint-config": "^5.0.0",
    "@loopback/testlab": "^1.10.0",
    "@types/node": "^10.17.17",
    "@types/passport": "1.0.2",
    "@types/passport-jwt": "3.0.3",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-eslint-plugin": "^2.1.0",
    "eslint-plugin-mocha": "^6.2.2",
    "source-map-support": "^0.5.16",
    "typescript": "~3.7.3"
  }
}
